package test;

public class test {
	public static void main(String[] args) {
		for(int i = 1; i < 10; i++) {
			int k = (i > 5) ? 10 - i : i;
			for(int j = 0; j < 5; j++) {
				if(k + j < 5) {
					System.out.print(" ");
				}
				else {
					System.out.print("*");
				}
			}
			System.out.println();
		}
	}
}
