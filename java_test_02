package test;

public class test {
	public static void main(String[] args) {
		for(int i = 1; i < 10; i++) {
			int j;
			int k;
			if(i < 5) {
				j = i;
				k = 5 - i;
			}
			else {
				j = 10 - i;
				k = i - 5;
			}
			while(k > 0) {
				System.out.print(" ");
				k--;
			}
			while(j > 0) {
				System.out.print("*");
				j--;
			}
			System.out.println();
		}
	}
}
